<AnchorPane prefHeight="500.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.FourthWindowController">
   <children>
      <Region layoutY="-10.0" prefHeight="118.0" prefWidth="400.0" style="-fx-background-color: #FFAB00;" />
      <Label alignment="CENTER" contentDisplay="CENTER" layoutY="20.0" text="Choose Level:" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
