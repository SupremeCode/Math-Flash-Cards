            <Font size="40.0" />
         </font>
      </Label>
      <VBox alignment="CENTER" layoutX="300.0" layoutY="144.0" prefHeight="200.0" prefWidth="100.0" spacing="20.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0">
         <children>
            <Button id="levelButton" fx:id="beginnerButton" mnemonicParsing="false" onAction="#beginner" prefWidth="175.0" stylesheets="@application.css" text="Beginner">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button id="levelButton" fx:id="intermediateButton" mnemonicParsing="false" onAction="#intermediate" prefWidth="175.0" stylesheets="@application.css" text="Intermediate">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button id="levelButton" fx:id="expertButton" mnemonicParsing="false" onAction="#expert" prefWidth="175.0" stylesheets="@application.css" text="Expert">
